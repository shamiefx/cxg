{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true
  },
  "hooks": {
    "predeploy": {
      "commands": [
        "npm run build:static"
      ]
    }
  }
}
